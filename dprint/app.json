{
	"lineWidth": 120,
	"useTabs": true,
	"prettier": {
		"associations": [
			"**/*.{css,scss}"
		],
		"parser": "scss",
		"printWidth": 120,
		"useTabs": true
	},
	"typescript": {
		"bracePosition": "nextLine",
		"jsx.bracketPosition": "nextLine",
		"jsx.multiLineParens": "prefer",
		"nextControlFlowPosition": "nextLine",
		"operatorPosition": "maintain",
		"preferHanging": false,
		"quoteProps": "asNeeded",
		"quoteStyle": "preferDouble",
		"semiColons": "always",
		"exportDeclaration.sortNamedExports": "maintain",
		"importDeclaration.sortNamedImports": "maintain",
		"tupleType.spaceAround": false,
		"typeAnnotation.spaceBeforeColon": false,
		"functionDeclaration.spaceBeforeParentheses": true,
		"functionExpression.spaceBeforeParentheses": true,
		"getAccessor.spaceBeforeParentheses": true,
		"method.spaceBeforeParentheses": true,
		"setAccessor.spaceBeforeParentheses": true,
		"exportDeclaration.spaceSurroundingNamedExports": false,
		"importDeclaration.spaceSurroundingNamedImports": false,
		"spaceSurroundingProperties": false,
		"module.sortExportDeclarations": "maintain",
		"module.sortImportDeclarations": "maintain",
		"useBraces": "always",
		"arrowFunction.useParentheses": "preferNone"
	},
	"json": {},
	"excludes": [
		"**/.next",
		"**/node_modules",
		"**/*-lock.json",
		"**/storybook-static"
	],
	"plugins": [
		"https://plugins.dprint.dev/json-0.19.4.wasm",
		"https://plugins.dprint.dev/typescript-0.93.2.wasm",
		"https://plugins.dprint.dev/prettier-0.46.1.json@e5bd083088a8dfc6e5ce2d3c9bee81489b065bd5345ef55b59f5d96627928b7a"
	]
}
